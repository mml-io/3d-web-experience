{
  "compilerOptions": {
    "noImplicitAny": true,
    "module": "esnext",
    "target": "es2022",
    "sourceMap": true,
    "esModuleInterop": true,
    "allowJs": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "noEmit": true,
    "baseUrl": ".",
    "incremental": true,
    "skipLibCheck": true
  }
}
