{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**"]
    },
    "pre:build": {
      "dependsOn": ["^pre:build"],
      "cache": true
    },
    "depcheck": {
      "cache": true
    },
    "test": {
      "cache": true,
      "outputs": ["test-results/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "cache": true
    },
    "lint-fix": {
      "cache": true
    },
    "link": {
      "cache": true
    },
    "type-check": {
      "cache": true
    },
    "iterate": {
      "cache": false
    },
    "start-server": {
      "cache": false
    }
  }
}
