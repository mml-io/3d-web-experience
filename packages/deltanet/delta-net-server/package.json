{
  "name": "@mml-io/delta-net-server",
  "version": "0.1.0",
  "publishConfig": {
    "access": "public"
  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "type": "module",
  "files": [
    "/build"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsx ./build.ts --build",
    "iterate": "tsx ./build.ts --watch",
    "lint": "eslint \"./{src,test}/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint-fix": "eslint \"./{src,test}/**/*.{js,jsx,ts,tsx}\" --fix",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "dependencies": {
    "@mml-io/delta-net-protocol": "^0.1.0",
    "pako": "2.1.0"
  },
  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@types/jest": "29.5.14",
    "@types/pako": "2.0.3",
    "jest": "29.7.0",
    "jest-junit": "16.0.0",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2"
  }
}
